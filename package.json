{
  "scripts": {
    "dev": "npm run server",
    "build:site": "vite build && vite build --ssr",
    "build:worker": "esbuild worker/index.js --bundle --minify --outfile=dist/_worker.js --format=esm",
    "build": "pnpm run build:site && pnpm run build:worker",
    "server": "ts-node ./server",
    "serve": "wrangler pages dev ./dist"
  },
  "resolve": {
    "@vue/server-renderer": "./vue-server-renderer"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "@vitejs/plugin-vue": "^1.10.1",
    "@vue/compiler-sfc": "^3.2.24",
    "@vue/server-renderer": "^3.2.24",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "vite": "2.7.3",
    "vite-plugin-ssr": "./vite-plugin-ssr",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "esbuild": "^0.14.5",
    "parcel": "^2.0.1",
    "rollup": "^2.61.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-externals": "^3.1.1",
    "rollup-plugin-node-globals": "^1.4.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "wrangler": "^0.0.0-beta.6"
  },
  "volta": {
    "node": "16.7.0"
  },
  "source": "worker/_worker.js",
  "targets": {
    "default": {
      "distDir": "./dist",
      "context": "web-worker",
      "outputFormat": "esmodule",
      "includeNodeModules": true
    }
  }
}
