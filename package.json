{
  "scripts": {
    "dev": "esno ./server",
    "build:site": "vite build && vite build --ssr",
    "build:worker": "esbuild worker/index.js --bundle --minify --outfile=dist/_worker.js --format=esm",
    "build": "pnpm run build:site && pnpm run build:worker",
    "serve": "wrangler pages dev ./dist"
  },
  "dependencies": {
    "vue": "^3.2.26"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.17",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/server-renderer": "^3.2.26",
    "esbuild": "^0.14.8",
    "esno": "^0.13.0",
    "express": "^4.17.2",
    "typescript": "^4.5.4",
    "vite": "2.7.6",
    "vite-plugin-ssr": "^0.3.40",
    "wrangler": "^0.0.6"
  },
  "volta": {
    "node": "16.7.0"
  }
}
